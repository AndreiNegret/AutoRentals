<script>function processForm(evt) {
        const form = evt.target;
        // find form data values
        var values = {};
        $.each($(form).serializeArray(), function (i, field) {
            if (field.name == 'pickDate' || field.name == 'returnDate') {
                values[field.name] = new Date(field.value).toISOString();
            } else {
                values[field.name] = field.value;
            }
        });
        $.ajax({
            url: 'BookCars/Create',
            method: 'POST',
            data: values,
            success: function () {
                window.location = '/BookCars';
            }
        })
        evt.preventDefault();
    }</script>