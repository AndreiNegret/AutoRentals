@model ASRental.Models.BookCar

@{
    ViewData["Title"] = "Home Page";
}

<!--== SlideshowBg Area Start ==-->
<section id="slideslow-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="slideshowcontent">
                    <div class="display-table">
                        <div class="display-table-cell">
                            <h1>BOOK A CAR TODAY!</h1>
                            <p>FOR AS LOW AS $40 A DAY PLUS 15% DISCOUNT <br> FOR OUR RETURNING CUSTOMERS</p>

                            <div class="book-ur-car">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="BookCarId" />
                                <form method="post" onsubmit="processForm(event)" class="d-flex">
                                    <div class="pick-location bookinput-item">
                                        <select asp-for="Location" name="location" placeholder="Location" required class="custom-select">
                                            <option selected>Pick Location</option>
                                            <option value="San Francisco">San Francisco</option>
                                            <option value="Los Angeles">Los Angeles</option>
                                            <option value="San Diego">San Diego</option>
                                            <option value="San Jose">San Jose</option>
                                            <option value="Fresno">Fresno</option>
                                            <option value="Sacramento">Sacramento</option>
                                        </select>
                                        <span asp-validation-for="Location" class="text-danger"></span>
                                    </div>

                                    <div class="pick-date bookinput-item">
                                        <input asp-for="PickDate" type="text" id="startDate" name="pickDate" placeholder="Pick Date" class="start-date" required />
                                    </div>

                                    <div class="return-date bookinput-item">
                                        <input asp-for="ReturnDate" type="text" id="endDate" placeholder="Return Date" name="returnDate" class="end-date" required />
                                    </div>

                                    <div class="car-choose bookinput-item">
                                        <select asp-for="CarName" type="text" name="carName" placeholder="Car Name" required class="custom-select">
                                            <option selected>Car Name</option>
                                            <option value="Cadillac">Cadillac</option>
                                            <option value="Audi">Audi</option>
                                            <option value="BMW">BMW</option>
                                            <option value="Mercedes-Benz">Mercedes-Benz</option>
                                            <option value="Porsche">Porsche</option>
                                            <option value="Jeep">Jeep</option>
                                        </select>
                                        <span asp-validation-for="CarName" class="text-danger"></span>
                                    </div>

                                    <div class="bookcar-btn bookinput-item">
                                        <input type="submit" value="Book Car" class="btn btn-primary" />
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--== SlideshowBg Area End ==-->
